# Ignore bundler config.
web/.bundle

# Ignore the default SQLite database.
web/db/*.sqlite3
db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
web/log/*
!web/log/.keep
web/tmp

web/.idea/
web/atlassian-ide-plugin.xml
.idea
.idea/*
